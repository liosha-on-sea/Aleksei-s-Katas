using System;

namespace MyApp
{
    internal class Program
    {
        static void Main(string[] args)
        {
            var result = StringRepeator("huy", 5);
        }

        static string StringRepeator(string txt, int times)
        {
            var result = txt;

            for (int i = 0; i <= times; i++)
            {
               result+= txt;
            }

            return result;  
        }
    }
}
